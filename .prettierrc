{
    "useTabs": false,
    "tabWidth": 4,
    "singleQuote": false,
    "trailingComma": "none",
    "printWidth": 100,
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-sort-imports"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "bracketSameLine": false,
    "htmlWhitespaceSensitivity": "ignore",
    "sortingMethod": "lineLength"
}